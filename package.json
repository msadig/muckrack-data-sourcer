{
  "name": "browser-automation-scraper",
  "version": "1.0.0",
  "description": "Simple browser automation project for web scraping",
  "main": "src/index.js",
  "type": "module",
  "scripts": {
    "start": "node src/index.js",
    "profiles": "node src/multilogin-scraper.js",
    "profiles:resume": "node src/multilogin-scraper.js --resume",
    "profiles:resume-headless": "node src/multilogin-scraper.js --resume --headless",
    "profiles:fresh": "node src/multilogin-scraper.js --fresh",
    "profiles:fresh-headless": "node src/multilogin-scraper.js --fresh --headless",
    "outlets": "node src/multilogin-outlet-scraper.js",
    "outlets:resume": "node src/multilogin-outlet-scraper.js --resume",
    "outlets:resume-headless": "node src/multilogin-outlet-scraper.js --resume --headless",
    "outlets:fresh": "node src/multilogin-outlet-scraper.js --fresh",
    "outlets:fresh-headless": "node src/multilogin-outlet-scraper.js --fresh --headless",
    "test:state": "node test-state-manager.js",
    "multilogin": "node src/multilogin-scraper.js",
    "multilogin-outlets": "node src/multilogin-outlet-scraper.js",
    "force-stop": "node src/force-stop-profile.js",
    "install-browsers": "playwright install chromium",
    "pm2:start": "pm2 start ecosystem.config.cjs",
    "pm2:start:profiles": "pm2 start ecosystem.config.cjs --only muckrack-profiles-scraper",
    "pm2:start:outlets": "pm2 start ecosystem.config.cjs --only muckrack-outlets-scraper",
    "pm2:stop": "pm2 stop ecosystem.config.cjs",
    "pm2:restart": "pm2 restart ecosystem.config.cjs",
    "pm2:delete": "pm2 delete ecosystem.config.cjs",
    "pm2:logs": "pm2 logs",
    "pm2:logs:profiles": "pm2 logs muckrack-profiles-scraper",
    "pm2:logs:outlets": "pm2 logs muckrack-outlets-scraper",
    "pm2:status": "pm2 status",
    "pm2:monit": "pm2 monit",
    "pm2:save": "pm2 save",
    "pm2:startup": "pm2 startup"
  },
  "keywords": ["scraping", "automation", "playwright"],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "playwright": "^1.40.0",
    "dotenv": "^16.3.1",
    "pm2": "^5.3.0"
  }
}
